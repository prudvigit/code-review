  case
    when eord.flifn <> '' and eord.febel <> ''
         and marc.beskz = 'F'
         and ( marc.sobsl = '' or marc.sobsl like '30%' )
         and eina.infnr is not null
         and eine.infnr is not null
      then 'Y'
    when eord.flifn <> '' and eord.febel <> ''
         and marc.beskz = 'F'
         and ( marc.sobsl = '' or marc.sobsl like '30%' )
         and ( eina.infnr is null or eine.infnr is null )
      then 'N'
    else ''
  end as PirIndicatorFlag,
